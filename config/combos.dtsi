/ {
    combos {
        compatible = "zmk,combos";

        CapsWord {
            bindings = <&caps_word>;
            key-positions = <12 13>;
            layers = <0>;
        };

        combo_BASE_or_EXTRAS {
            bindings = <&td_bore>;
            key-positions = <32 33>;
        };
        
        combo_left_bracket {
            bindings = <&kp LEFT_BRACKET>;
            key-positions = <6 7>;
        };
        
        combo_right_bracket {
            bindings = <&kp RIGHT_BRACKET>;
            key-positions = <7 8>;
        };
        
        combo_backslash {
            bindings = <&kp BACKSLASH>;
            key-positions = <8 9>;
        };
    };
};