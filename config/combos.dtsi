/ {
    combos {
        compatible = "zmk,combos";

        CapsWord {
            bindings = <&caps_word>;
            key-positions = <12 13>;
            layers = <0>;
        };

        combo_BASE_or_EXTRAS {
            bindings = <&td_bore>;
            key-positions = <32 33>;
        };
        
        combo_left_bracket {
            bindings = <&kp LEFT_BRACKET>;
            key-positions = <7 8>;
        };
        
        combo_slash {
            bindings = <&kp SLASH>;
            key-positions = <8 9>;
        };
        
        combo_single_quote {
            bindings = <&kp SINGLE_QUOTE>;
            key-positions = <9 10>;
        };
    };
};